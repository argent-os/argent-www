    <script>
        document.querySelector('#send_link').onclick = function(e){
            e.preventDefault()
            swal({
                title: "Please enter email",
                text: "We'll send you a link to the app!",
                type: 'input',
                showCancelButton: true,
                closeOnConfirm: false,
                animation: "slide-from-bottom",
                inputPlaceholder: "john@example.com",
            },
            function(inputValue){
                if (inputValue === false) return false;

                if (inputValue === "") {
                    swal.showInputError("Empty email not valid");
                    return false;
                }
            
                sendEmail(inputValue)

            });
        };
    </script>
    <script>
        function sendEmail(email) {
            //console.log("submitting")
            var form = $('#emailForm');
            // var email = $('input[name="Email"]').val()
            if (email === "") {
                swal({   
                    title: "Error",   
                    text: "Email field cannot be empty",   
                    type: "error",   
                    confirmButtonText: "Ok" 
                });
                return;
            }
            // console.log("phone number is", "+1" + rawPhoneNumber);
            // console.log(form)
            $.ajax({
                type: "POST",
                url: "https://api.argent.cloud/v1/email",
                dataType: "json",  
                contentType: 'application/json; charset=UTF-8',
                data: JSON.stringify({
                    "email": email
                }),
                success: function (data) {
                    // Send a "Played song" event to Mixpanel
                    // with a property "genre"
                    mixpanel.track(
                        "Email app sent",
                        {"status": "success"}
                    );
                    swal({   
                        type:"success",
                        title: "Success",   
                        text: "The app link is on it's way!",  
                        confirmButtonText: "Great!" 
                    });
                },
                failure: function(err) {
                    mixpanel.track(
                        "Email app error",
                        {"status": "error"}
                    );                    
                    swal({   
                        title: "Error",   
                        text: "App link could not be sent to email",   
                        type: "error",   
                        confirmButtonText: "Ok" 
                    });
                }
            });
        }
    </script>
    <script type="text/javascript">
        $(document).ready(function(){
            /* Get iframe src attribute value i.e. YouTube video url
            and store it in a variable */
            var url = $("#video").attr('src');
            
            /* Assign empty url value to the iframe src attribute when
            modal hide, which stop the video playing */
            $("#modal").on('hide.bs.modal', function(){
                $("#video").attr('src', '');
            });
            
            /* Assign the initially stored url back to the iframe src
            attribute when modal is displayed again */
            $("#modal").on('show.bs.modal', function(){
                $("#video").attr('src', url);
                // Send a "Played song" event to Mixpanel
                // with a property "genre"
                mixpanel.track(
                    "Played video",
                    {"video": "argent-intro"}
                );
            });
        });
    </script>    
